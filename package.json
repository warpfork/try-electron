{
  "name": "try-electron35",
  "version": "0.0.1",
  "description": "Hello World!",
  "scripts": {
    "prep": "bun install",
    "build": "bun build ./src/*.js --target=node --outdir=./dist/",
    "watch": "bun build ./src/*.js --target=node --outdir=./dist/ --watch",
    "start": "rm -rf dist/* && bun run build && time ./lib/electron/electron ./dist/main.js"
  },
  "type": "module",
  "devDependencies": {},
  "dependencies": {},
  "peerDependencies": {
    "electron": "^35",
    "typescript": "^5.0.0"
  }
}
