{
  "name": "try-electron35",
  "version": "0.0.1",
  "description": "Hello World!",
  "scripts": {
    "prep": "ELECTRON_SKIP_BINARY_DOWNLOAD=1 bun install",
    "build": "bun run build:most && bun run build:preload",
    "build:most": "bun build ./src/{main,renderer}.ts --external=electron --external=require --target=node --outdir=./dist/",
    "build:preload": "bun build ./src/preload.ts --external=electron --external=require --target=node --format=cjs --outdir=./dist/",
    "start": "rm -rf dist/* && bun run build && time ../lib/electron/electron ./dist/main.js"
  },
  "type": "module",
  "devDependencies": {
    "electron": "^35"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
